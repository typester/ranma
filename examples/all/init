#!/bin/zsh

# Launch all example plugins at once.

# Homebrew PATH (not needed if already set in ~/.zshenv)
export PATH="/opt/homebrew/bin:$PATH"

SCRIPT_DIR="${0:A:h}"

source "$SCRIPT_DIR/../clock/init"
source "$SCRIPT_DIR/../battery/init"
source "$SCRIPT_DIR/../now-playing/init"
source "$SCRIPT_DIR/../cpu-mem/init"
source "$SCRIPT_DIR/../wifi/init"
source "$SCRIPT_DIR/../mic-camera/init"
